<!DOCTYPE html>
<html>
  <head>
    <title>Health</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-inverse">
    <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbarEexample7">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <nav class="nav navbar-nav navbar-left">
      <span class="navbar-brand">
        Health
      </span>
    </div>

    <div class="collapse navbar-collapse" id="navbarEexample7">
      <ul class="nav navbar-nav navbar-right">
        <% if user_signed_in? %>
        <li class="">
        <%= link_to "Home", root_path, class: 'glyphicon glyphicon-home' %>
        </li>
        <li class="">
        <%= link_to "About", home_about_path, class: 'glyphicon glyphicon-link' %>
        </li>
        <li class="">
        <%= link_to "Topics", topics_path, class: 'glyphicon glyphicon-book' %>
        </li>
        <li class="">
        <%= link_to "Logout", destroy_user_session_path, class: 'glyphicon glyphicon-log-out', method: :delete %>
        </li>
        <% else %>
        <li class="">
        <%= link_to 'Home' , root_path, class: 'glyphicon glyphicon-home'%>
        </li>
        <li class="">
        <%= link_to 'About', home_about_path, class: 'glyphicon glyphicon-link' %>
        </li>
        <li class="">
        <%= link_to 'sign up', new_user_registration_path, class: 'glyphicon glyphicon-edit' %>
        </li>
        <li class="">
        <%= link_to "login", new_user_session_path, class: 'glyphicon glyphicon-log-out' %>
        </li>
        <% end %>
      </ul>
    </div>
    </div>
    </nav>



    <%= yield %>
  </body>
</html>
